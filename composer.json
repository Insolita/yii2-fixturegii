{
    "name": "insolita/yii2-fixturegii",
    "description": "gii fixture helper - generate fixture classes; faker templates; fixture data files from table",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "MIT",
	"version": "1.0.2",
    "authors": [
        {
            "name": "insolita",
            "email": "webmaster100500@ya.ru"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
		"yiisoft/yii2-gii": "2.0.*",
		"insolita/yii2-validatorpack":"~0.0.1"
    },
	"require-dev":{
		"vlucas/phpdotenv": "*",
		"codeception/codeception": "^2.2.3",
		"codeception/verify": "~0.3.1",
		"codeception/specify": "~0.4.3",
		"nesbot/carbon": "~1.9"
	},
	"suggest": {
		"insolita/yii2-skeletest": "1.*"
	},
    "autoload": {
        "psr-4": {
            "insolita\\fixturegii\\": "src/"
        }
    }
}
