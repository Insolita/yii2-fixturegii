{
    "name": "insolita/yii2-fixturegii",
    "description": "gii tool for generate fixture templates by table schema",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "MIT",
    "authors": [
        {
            "name": "insolita",
            "email": "donnainsolita@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
		"insolita/yii2-validatorpack":"~0.0.1"
    },
	"require-dev":{
		"yiisoft/yii2-faker": "*"
	},
    "autoload": {
        "psr-4": {
            "insolita\\fixturegii\\": ""
        }
    },
	"extra": {
	   "bootstrap": "insolita\\fixturegii\\Bootstrap"
	}
}
